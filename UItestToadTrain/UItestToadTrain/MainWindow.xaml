<Window x:Name="RoadTrainMain" x:Class="UItestToadTrain.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UItestToadTrain"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" KeyDown="RoadTrainMain_KeyDown">
    <Grid x:Name="grid">
        <Label Content="Label" HorizontalAlignment="Left" Margin="86,-22,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="pointTextBox" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
